<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <data>

    <record id="report_paperformat_a4_portrait" model="report.paperformat">
      <field name="name">Paper Format A4</field>
      <field name="default" eval="True"/>
      <field name="format">A4</field>
      <field name="page_height">0</field>
      <field name="page_width">0</field>
      <field name="orientation">Portrait</field>
      <field name="margin_top">40</field>
      <field name="margin_bottom">19</field>
      <field name="margin_left">15</field>
      <field name="margin_right">10</field>
      <field name="header_line" eval="False"/>
      <field name="header_spacing">35</field>
      <field name="dpi">90</field>
    </record>

    <record id="report_soh_all_shop_action" model="ir.actions.report">
      <field name="name">Stock On Hand All Shop</field>
      <field name="model">stock.quant</field>
      <field name="report_type">xlsx</field>
      <field name="report_name">sol_stock.report_soh_all_shop_xlsx</field>
      <field name="report_file">sol_stock.report_soh_all_shop_xlsx</field>
      <field name="print_report_name">'Stock On Hand All Shop'</field>
      <field name="binding_model_id" ref="stock.model_stock_quant"/>
      <field name="binding_type">report</field>
    </record>

    <record id="stock_per_store_action_xlsx" model="ir.actions.report">
      <field name="name">Stock On Hand Per Store</field>
      <field name="model">stock.per.store.wizard</field>
      <field name="report_type">xlsx</field>
      <field name="report_name">sol_stock.report_soh_per_shop_xlsx</field>
      <field name="report_file">sol_stock.report_soh_per_shop_xlsx</field>
      <field name="binding_type">report</field>
    </record>

    <record id="action_report_internal_action" model="ir.actions.report">
      <field name="name">Report - Internal Transfer</field>
      <field name="model">stock.picking</field>
      <field name="report_type">qweb-pdf</field>
      <field name="report_name">sol_stock.report_internal_type</field>
      <field name="report_file">sol_stock.report_internal_type</field>
      <field name="paperformat_id" ref="sol_stock.report_paperformat_a4_portrait"/>
      <field name="print_report_name">'INTERNAL TRANSFER - %s - %s' % (object.partner_id.name or '', object.name)</field>
      <field name="binding_model_id" ref="stock.model_stock_picking"/>
<!--      <field name="binding_model_id" eval="False"/>-->
      <field name="binding_type">report</field>
      <field name="groups_id" eval="[(4, ref('sol_stock.group_report_internal'))]"/>
    </record>

    <record id="action_report_receive_action" model="ir.actions.report">
      <field name="name">Report - Receipts</field>
      <field name="model">stock.picking</field>
      <field name="report_type">qweb-pdf</field>
      <field name="report_name">sol_stock.report_receive_type</field>
      <field name="report_file">sol_stock.report_receive_type</field>
      <field name="paperformat_id" ref="sol_stock.report_paperformat_a4_portrait"/>
      <field name="print_report_name">'RECEIPTS - %s - %s' % (object.partner_id.name or '', object.name)</field>
      <field name="binding_model_id" ref="stock.model_stock_picking"/>
<!--      <field name="binding_model_id" eval="False"/>-->
      <field name="binding_type">report</field>
      <field name="groups_id" eval="[(4, ref('sol_stock.group_report_receipts'))]"/>
    </record>

    <record id="action_report_return_action" model="ir.actions.report">
      <field name="name">Report - Return</field>
      <field name="model">stock.picking</field>
      <field name="report_type">qweb-pdf</field>
      <field name="report_name">sol_stock.report_return_type</field>
      <field name="report_file">sol_stock.report_return_type</field>
      <field name="paperformat_id" ref="sol_stock.report_paperformat_a4_portrait"/>
      <field name="print_report_name">'RETURN - %s - %s' % (object.partner_id.name or '', object.name)</field>
      <field name="binding_model_id" ref="stock.model_stock_picking"/>
<!--      <field name="binding_model_id" eval="False"/>-->
      <field name="binding_type">report</field>
      <field name="groups_id" eval="[(4, ref('sol_stock.group_report_return'))]"/>
    </record>

    <record id="action_report_production_stock" model="ir.actions.report">
      <field name="name">Report - Purchase Notes</field>
      <field name="model">stock.picking</field>
      <field name="report_type">qweb-pdf</field>
      <field name="report_name">sol_stock.report_production</field>
      <field name="report_file">sol_stock.report_production</field>
      <field name="paperformat_id" ref="sol_stock.report_paperformat_a4_portrait"/>
      <field name="print_report_name">'PRODUCTION - %s - %s' % (object.partner_id.name or '', object.name)</field>
      <field name="binding_model_id" ref="stock.model_stock_picking"/>
      <field name="binding_type">report</field>
      <field name="groups_id" eval="[(4, ref('sol_stock.group_report_production'))]"/>
    </record>

    <record id="stock.action_report_picking" model="ir.actions.report">
        <field name="binding_model_id" eval="False"/>
    </record>
    <record id="stock.action_report_delivery" model="ir.actions.report">
        <field name="binding_model_id" eval="False"/>
    </record>

    <record id="action_report_testing_action" model="ir.actions.report">
      <field name="name">Report - Testing</field>
      <field name="model">stock.picking</field>
      <field name="report_type">qweb-pdf</field>
      <field name="report_name">sol_stock.report_testing</field>
      <field name="report_file">sol_stock.report_testing</field>
      <field name="paperformat_id" ref="sol_stock.report_paperformat_a4_portrait"/>
      <field name="print_report_name">'Testing - %s - %s' % (object.partner_id.name or '', object.name)</field>
      <field name="binding_model_id" ref="stock.model_stock_picking"/>
<!--      <field name="binding_model_id" eval="False"/>-->
      <field name="binding_type">report</field>
<!--      <field name="groups_id" eval="[(4, ref('sol_stock.group_report_internal'))]"/>-->
    </record>


  </data>
  

</odoo>
