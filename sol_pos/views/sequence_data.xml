<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_stock_needs" model="ir.sequence">
            <field name="name">Sequence Stock Needs</field>
            <field name="code">stock.needs.seq</field>
            <field name="padding">6</field>
            <field name="prefix">SN</field>
            <field name="company_id" eval="2"/>
        </record>

        <record id="sequence_stock_comment" model="ir.sequence">
            <field name="name">Sequence Stock Comments</field>
            <field name="code">stock.comment.seq</field>
            <field name="padding">6</field>
            <field name="prefix">SC</field>
            <field name="company_id" eval="2"/>
        </record>



    </data>
</odoo>