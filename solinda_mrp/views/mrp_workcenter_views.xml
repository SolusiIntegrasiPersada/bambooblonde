<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="mrp_workcenter_inherit_product_service_id" model="ir.ui.view">
        <field name="name">mrp.workcenter.inherit.product.service.id</field>
        <field name="model">mrp.workcenter</field>
        <field name="inherit_id" ref="mrp.mrp_workcenter_view"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page/group/group/field[@name='costs_hour']" position="after">
                <field name="product_service_id" />
            </xpath>

            <xpath expr="//field[@name='name']" position="after">
                <field name="is_dyeing"/>
            </xpath>
            
        </field>
    </record>

</odoo>
